{
    "routes": [
        {
            "route": "/*",
            "rewrite": "/index.html"
        }
    ],
    "navigationFallback": {
        "rewrite": "/index.html",
        "exclude": [
            "/images/*.{png,jpg,gif}",
            "/css/*",
            "/_next/*"
        ]
    },
    "mimeTypes": {
        ".json": "application/json"
    }
}